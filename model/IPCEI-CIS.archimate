        </children>
      </children>
      <children
          xsi:type="archimate:DiagramModelGroup"
          name="Participant Domain"
          id="id-view-node-part-domain"
          textAlignment="1"
          fillColor="#ffffff"
          bounds="600,20,560,400">
        <archimateElement
            xsi:type="archimate:Grouping"
            href="id-participant-domain"/>
        <children
            xsi:type="archimate:DiagramModelGroup"
            name="Governance Layer"
            id="id-view-node-gov-layer-part"
            textAlignment="1"
            fillColor="#d3e3f3"
            bounds="20,20,520,100">
          <archimateElement
              xsi:type="archimate:Grouping"
